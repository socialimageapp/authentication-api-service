{
	"type": "module",
	"private": true,
	"scripts": {
		"build:submodules": "tsx .config/scripts/buildSubmodules.ts",
		"upgradeAst": "cd lib/ast && pnpm install && pnpm build && cd ../../ && pnpm upgrade @adventurai/shared-types",
		"knip": "knip",
		"depCheck:svg": "npx depcruise src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg",
		"depCheck": "npx depcruise src --include-only \"^src\"",
		"build": "pnpm run build:submodules && tsc --project tsconfig.build.json",
		"start": "node dist/index.js",
		"dev": "nodemon src/index.ts",
		"tsPatch": "ts-patch install -s",
		"package": "mkdir -p production && cp package.json production/ && cp -R dist production/ && cp -R public production/ && pnpm install --prod --dir ./production",
		"test:types": "tsc --project tsconfig.json --noEmit",
		"test": "pnpm test:types",
		"auto:upsub": "tsx .config/scripts/updateRepos.ts",
		"test:mocha": "mocha  --import=tsx/esm 'src/**/*.test.ts'",
		"lint": "cross-env eslint -c eslint.config.mjs",
		"email:build": "email build",
		"email:dev": "email dev --dir src/emails",
		"email:export": "email export"
	},
