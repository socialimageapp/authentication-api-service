{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"lib": ["ES2023"],
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"baseUrl": ".",
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"declarationMap": true,
		"alwaysStrict": true,
		"paths": {},
		"forceConsistentCasingInFileNames": true
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules", "dist"]
}
