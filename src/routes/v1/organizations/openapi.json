{
	"paths": {
		"/organizations": {
			"get": {
				"summary": "Get Organizations",
				"tags": ["Organizations"],
				"responses": {
					"200": {
						"description": "A list of users",
						"content": {
							"application/json": {
								"schema": {
									"type": "array",
									"items": {
										"type": "object",
										"properties": {
											"id": {
												"type": "integer",
												"example": 1
											},
											"name": {
												"type": "string",
												"example": "John Doe"
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
